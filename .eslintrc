{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, {
            "extensions": [".js", ".jsx"]
        }],
        "linebreak-style": 0
    },
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "document": false
    }
}